if ( $('#videoSolo').length ){
	
	// Get current plays_count value from database & update video tile icon value
	var $video_id = $.cookie("video_id");
	$.get($video_id, function(data) {
	  $('.plays').text(data.plays_count);
	}, "json");
	
} else {
	
	$('#play_video_id').val('');

	// Get current plays_count value from database & update video tile icon values
	var $video_id = $.cookie("video_id");
	$.get('videos/'+$video_id+'', function(data) {
	  $('*[data-video-id="'+$video_id+'"]').find('.plays').text(data.plays_count);
	}, "json");
	
};
